# Основные настройки
APP_NAME=voy-api
PORT=8080
APP_MODE=development

# Порты сервисов
AUTH_PORT=8080
PROFILE_PORT=8081
ORCHESTRATOR_PORT=8082

# База данных
DB_HOST=localhost
DB_PORT=5432
DB_NAME=voy_db
DB_USER=postgres
DB_PASS=admin
DB_SSL_MODE=disable

# Аутентификация
JWT_SECRET=development-jwt-secret-key-change-me
TOKEN_EXPIRATION=24h
COOKIE_DOMAIN=
COOKIE_SECURE=false
SESSIONS_CLEANUP=1h

# CORS настройки
CORS_ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173,http://127.0.0.1:3000,http://localhost:5174

# Внешние сервисы
WHISPER_API_KEY=your-whisper-api-key
LLM_API_URL=https://api.openai.com/v1
LLM_API_KEY=your-openai-api-key 